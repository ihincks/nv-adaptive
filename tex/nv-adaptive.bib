  url = {https://www.nature.com/articles/nnano.2012.50},
  journal = {Nature Nanotechnology},
  author = {Maletinsky, P. and Hong, S. and Grinolds, M. S. and Hausmann, B. and Lukin, M. D. and Walsworth, R. L. and Loncar, M. and Yacoby, A.},
  month = may,
  year = {2012},
  pages = {320-324},
  file = {/home/ihincks/Zotero/storage/4V9J2ZYV/Maletinsky et al. - 2012 - A robust scanning diamond sensor for nanoscale ima.pdf;/home/ihincks/Zotero/storage/NP53PYFG/nnano.2012.html}
