  url = {https://link.aps.org/doi/10.1103/PhysRevA.80.052114},
  journal = {Physical Review A},
  author = {Berry, D. W. and Higgins, B. L. and Bartlett, S. D. and Mitchell, M. W. and Pryde, G. J. and Wiseman, H. M.},
