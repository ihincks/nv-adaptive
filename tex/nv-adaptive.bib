  month = apr,
  year = {2013},
  pages = {215-219},
  file = {/home/ihincks/Zotero/storage/EGYX27A4/Grinolds et al. - 2013 - Nanoscale magnetic imaging of a single electron sp.pdf;/home/ihincks/Zotero/storage/TIEEISHZ/nphys2543.html}
